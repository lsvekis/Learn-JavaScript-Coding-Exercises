let num = 10;
// Arithmetic operations
num += 5;   // num = num + 5
console.log("After += 5, num =", num);
num -= 3;   // num = num - 3
console.log("After -= 3, num =", num);
num *= 2;   // num = num * 2
console.log("After *= 2, num =", num);
num /= 4;   // num = num / 4
console.log("After /= 4, num =", num);
num **= 2;  // num = num ** 2 (exponentiation)
console.log("After **= 2, num =", num);
num %= 5;   // num = num % 5 (remainder)
console.log("After %= 5, num =", num);
